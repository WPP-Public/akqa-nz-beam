#!/usr/local/bin/php -q
<?php

require_once __DIR__ . '/../../../autoload.php';

use Heyday\Component\Beam\Command\Beam;
use Heyday\Component\Beam\Command\GenerateDeploy;
use Symfony\Component\Console\Application;

$application = new Application();
$application->add(new Beam());
$application->add(new GenerateDeploy());
$application->run();
